// Given d i s c r e t e s y s t em y (n)= x (n+7)
 clear ;
 clc ;
 x1 = [1 ,1 ,1 ,1];
x2 = [2 ,2 ,2 ,2];
 a = 1;
 b = 1;
 for t = 1: length ( x1 )
 x3 ( n ) = a * x1 ( n ) + b* x2 ( n ) ;
end
 for t = 1: length ( x1 )
 y1 (n+7) = n * x1 ( n ) ;
 y2 (n+7) = n * x2 ( n ) ;
 y3 (n+7) = n * x3 ( n ) ;
 end
 for n = 1: length ( y1 )
z ( n ) = a* y1 ( n ) + b * y2 ( n ) ;
 end
count = 0;
 for n =1: length ( y1 )
if( n3 ( n ) == z ( n ) )
 count = count +1;
end
end
if( count == length ( y3 ) )
end
